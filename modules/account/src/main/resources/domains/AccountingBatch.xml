<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  https://axelor.com/xml/ns/domain-models/domain-models_6.0.xsd">
  
  <module package="com.axelor.account.db" name="account"/>
  <entity name="AccountingBatch">
      <string name="code" title="Code" required="true"  />
      <integer name="actionSelect" required="true" title="Action" selection="accounting.batch.select"/>
      <one-to-many ref="com.axelor.account.db.Batch" name="batchList" title="Batches" mappedBy="accountingBatch"/>
  </entity>
 </domain-models>