<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views
    https://axelor.com/xml/ns/object-views/object-views_6.0.xsd">

	<form id="account-event-from" model="com.axelor.contact.db.Event"
		title="Events" name="event-form" extension="true">
		<extend
			target="//panel[@name='eventPanel']//field[@name='endDateT']">
			<insert position="after">
				<field name="duration" />
			</insert>
		</extend>
		<extend target="//field[@name='notes']">
			<attribute name="colSpan" value="12" />
		</extend>
	</form>

</object-views>